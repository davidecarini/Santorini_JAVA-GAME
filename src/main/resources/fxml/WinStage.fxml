<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="winPane" prefHeight="305.0" prefWidth="347.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1">

   <stylesheets>
      <URL value="@../css/Score.css" />
   </stylesheets>

   <children>

      <ImageView fitHeight="305.0" fitWidth="348.0">
         <image>
            <Image url="@../images/win/Odyssey_Win.png" />
         </image>
      </ImageView>

      <ImageView fitHeight="118.0" fitWidth="116.0" layoutX="116.0" layoutY="70.0">
         <image>
            <Image url="@../images/win/trophy.png" />
         </image>
      </ImageView>

      <Label layoutX="75.0" layoutY="32.0" text="YOU WIN!" />

      <Button fx:id="btnRejoin" layoutX="133.0" layoutY="194.0" mnemonicParsing="false" prefHeight="36.0" prefWidth="82.0" text="REJOIN" />
      <Button fx:id="btnQuit" layoutX="133.0" layoutY="241.0" mnemonicParsing="false" prefHeight="36.0" prefWidth="82.0" text="QUIT" />

   </children>
</AnchorPane>
